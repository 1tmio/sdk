{
  "name": "@rarible/connector",
  "version": "0.5.0",
  "license": "ISC",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "yarn run build-old",
    "build-babel": "babel --extensions \".ts,.tsx,.mjs\" ./src --out-dir build && yarn run build-old && yarn run replace-mjs",
    "build-old": "tsc --project tsconfig-build.json",
    "clean": "rimraf ./build",
    "replace-mjs": "find ./build -type f -name \"*.js\" -exec sed -i '' 's#./jsbi.mjs#jsbi#' {} \\;",
    "verify": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "jsbi": "^3.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/plugin-transform-typescript": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.14.5",
    "@rixio/atom": "~0.3.9",
    "rxjs": "6.6.7",
    "ts-jest": "^27.0.3",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@rixio/atom": "~0.3.9",
    "rxjs": "6.6.7",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
